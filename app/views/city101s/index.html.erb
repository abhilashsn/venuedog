<h1>Listing city101s</h1>

<table>
  <tr>
    <th>City</th>
    <th>Zip</th>
    <th>Description</th>
    <th>Links</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @city101s.each do |city101| %>
  <tr>
    <td><%= city101.city %></td>
    <td><%= city101.zip %></td>
    <td><%= city101.description %></td>
    <td><%= city101.links %></td>
    <td><%= link_to 'Show', city101 %></td>
    <td><%= link_to 'Edit', edit_city101_path(city101) %></td>
    <td><%= link_to 'Destroy', city101, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New City101', new_city101_path %>
