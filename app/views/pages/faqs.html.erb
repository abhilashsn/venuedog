<div id="content" class="page">

  <%= render "/shared/breadcrumbs" %>

  <h2><%= @page.title %></h2>

  <%= @page.content.html_safe if !@page.content.blank? %>

  <%= clear %>

</div>

<%= render "/shared/sidebar" %>




<% content_for :page_title, @page.title %>

<%= render :partial => 'shared/default_opengraph', :locals => {:current_page_title => @page.title } %>
